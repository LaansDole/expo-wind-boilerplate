{
  "name": "my-expo-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "concurrently \"tailwindcss -i input.css --postcss postcss.config.js --watch\" \"expo start\"",
    "tunnel": "concurrently \"tailwindcss -i input.css --postcss postcss.config.js --watch\" \"expo start --tunnel\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/config": "^8.5.4",
    "@expo/metro-config": "^0.17.5",
    "@expo/metro-runtime": "~3.1.3",
    "@expo/ngrok": "^4.1.0",
    "@expo/vector-icons": "^14.0.0",
    "@expo/webpack-config": "~19.0.1",
    "expo": "~50.0.8",
    "expo-image-picker": "~14.7.1",
    "expo-status-bar": "~1.11.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-web": "~0.19.6",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.35"
  },
  "private": true
}